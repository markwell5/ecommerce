{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Service for managing products",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "nodemon --exec babel-node dist/index.js"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "keywords": [
    "service",
    "product"
  ],
  "author": "Lewis",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "sqlite3": "^4.1.0",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.12",
    "@types/sqlite3": "^3.1.5",
    "nodemon": "^1.19.3",
    "tslint": "^5.20.0"
  }
}
